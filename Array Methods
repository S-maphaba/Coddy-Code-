using System;


public class Program {
   public static string[] Merge(string[] arr1, string[] arr2) {
    int allLength = arr1.Length + arr2.Length; //length of both arrays
    string[] newarr = new string[allLength]; //new array of length of both arrays

    // Copy first array
    Array.Copy(arr1, 0, newarr, 0, arr1.Length);

    // Copy second array after first array
    Array.Copy(arr2, 0, newarr, arr1.Length, arr2.Length);

    // Sort merged array
    Array.Sort(newarr);

    return newarr;
}

    public static void Main(string[] args) {
        string textArr1 = Console.ReadLine();
        string textArr2 = Console.ReadLine();
        string[] arr1 = textArr1.Split(",");
        string[] arr2 = textArr2.Split(",");

        string[] mergedArray = Merge(arr1, arr2);
        Console.WriteLine(string.Join(", ", mergedArray));
    }
}
